{
  "name": "RN-Production",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint-staged": "lint-staged",
    "android": "react-native run-android",
    "preios": "NODE_ENV='dev' node rn.config.js",
    "ios": "react-native run-ios --simulator='iPhone 12'",
    "preios:mock": "NODE_ENV=dev MOCK=true node rn.config.js",
    "ios:mock": "react-native run-ios --simulator='iPhone 12'",
    "build:android": "rm -f ./bin/rn-android.log && ./bin/android-build.sh ${env} ${version} ${versionName} ${preVersion} ${preVersionName}> ./bin/rn-android.log 2>&1 $2 $3 $4 $5",
    "build:ios": "rm -f ./bin/rn-ios.log && ./bin/ios-build.sh ${env} ${version} ${versionName} ${preVersion} ${preVersionName} > ./bin/rn-ios.log 2>&1 $2 $3 $4 $5",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .jsx,.ts,.tsx",
    "prettier": "prettier --write  --print-width 100 \"src/**/*.{js,ts,tsx}\"",
    "format": "yarn lint --fix",
    "tslint": "tsc --project ./tsconfig.json --noEmit",
    "start-cd": "node ./bin/app.js"
  },
  "pre-commit": [
    "format"
  ],
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.14.1",
    "@react-native-clipboard/clipboard": "1.7.0",
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-picker/picker": "^2.4.2",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/stack": "^6.2.2",
    "@rematch/core": "^2.2.0",
    "@types/react-redux": "^7.1.24",
    "dayjs": "1.10.4",
    "jcore-react-native": "1.9.4",
    "jpush-react-native": "2.8.8",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "4.0.3",
    "md5": "2.3.0",
    "react": "18.0.0",
    "react-native": "0.68.2",
    "react-native-animatable": "1.3.3",
    "react-native-camera": "3.43.0",
    "react-native-exit-app": "^1.1.0",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-image-picker": "3.2.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-ok-gesture-password": "2.0.0",
    "react-native-pager-view": "^5.2.1",
    "react-native-pay-password": "^1.0.1",
    "react-native-permissions": "^3.0.1",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0",
    "react-native-shadow": "^1.2.2",
    "react-native-simple-biometrics": "^1.5.0-alpha.1",
    "react-native-smart-refresh": "^1.1.6",
    "react-native-sms-verifycode-password": "^1.0.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.0.1",
    "react-native-ui-lib": "^6.18.0",
    "react-native-webview": "^11.23.0",
    "react-native-wechat-lib": "1.1.25-notpay",
    "react-redux": "^7.2.8",
    "redux-logger": "^3.0.6",
    "rn-vconsole-panel": "^1.0.2-bate1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/runtime": "^7.18.9",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^27.4.0",
    "@types/md5": "^2.3.2",
    "@types/react": "^18.0.15",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "3.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "4.14.1",
    "babel-jest": "^28.1.3",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-redux": "^3.3.4",
    "husky": "^8.0.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.5.4",
    "metro-react-native-babel-preset": "^0.59.0",
    "pre-commit": "1.2.2",
    "react-test-renderer": "^16.13.1",
    "typescript": "^4.5.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
